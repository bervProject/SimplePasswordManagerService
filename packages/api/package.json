{
  "name": "@bervproject/spm-services",
  "version": "0.0.1",
  "description": "Simple Password Manager Service",
  "keywords": [
    "feathers"
  ],
  "homepage": "https://github.com/bervProject/SimplePasswordManagerService",
  "bugs": {
    "url": "https://github.com/bervProject/SimplePasswordManagerService/issues",
    "email": "bervianto.leo@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Bervianto Leo Pratama",
    "email": "bervianto.leo@gmail.com"
  },
  "contributors": [],
  "main": "src",
  "types": "lib/",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "clean": "shx rm -rf lib/",
    "compile": "tsc",
    "build": "yarn run clean && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --no-notify src/",
    "fix-lint": "eslint --quiet --fix \"src/**/*\" \"test/**/*\"",
    "lint": "eslint \"src/**/*\" \"test/**/*\"",
    "migrate": "yarn run build && node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "yarn run build && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "yarn run build && node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "start": "yarn run build && cross-env NODE_ENV=production node lib/",
    "start:prod": "node lib/",
    "test": "jest --forceExit"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.159.0",
    "@bervproject/feathers-advance-hook": "^1.1.1",
    "@feathersjs/authentication": "^4.5.15",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.5.15",
    "@feathersjs/authentication-oauth": "^4.5.15",
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.12",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^6.1.4",
    "feathers-memory": "^4.1.0",
    "feathers-sequelize": "^6.3.4",
    "helmet": "^6.0.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.8",
    "pg": "^8.8.0",
    "sequelize": "^6.21.4",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nodemailer": "^6.4.5",
    "@types/sequelize": "^4.28.14",
    "@types/serve-favicon": "^2.5.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.5",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "prettier-plugin-packagejson": "^2.2.15",
    "sequelize-cli": "^6.4.1",
    "shx": "^0.3.4",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": "^16.0.0 || ^18.0.0",
    "yarn": ">=0.18.0"
  }
}