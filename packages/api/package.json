{
  "name": "@bervproject/spm-services",
  "version": "0.0.1",
  "description": "Simple Password Manager Service",
  "keywords": [
    "feathers"
  ],
  "homepage": "https://github.com/bervProject/SimplePasswordManagerService",
  "bugs": {
    "url": "https://github.com/bervProject/SimplePasswordManagerService/issues",
    "email": "bervianto.leo@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Bervianto Leo Pratama",
    "email": "bervianto.leo@gmail.com"
  },
  "contributors": [],
  "main": "src",
  "types": "lib/",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "clean": "shx rm -rf lib/",
    "compile": "tsc",
    "build": "yarn run clean && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --no-notify src/",
    "fix-lint": "eslint --quiet --fix \"src/**/*\" \"test/**/*\"",
    "lint": "eslint \"src/**/*\" \"test/**/*\"",
    "migrate": "yarn run build && node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "yarn run build && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "yarn run build && node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "start": "yarn run build && cross-env NODE_ENV=production node lib/",
    "start:prod": "node lib/",
    "test": "jest --forceExit"
  },
  "dependencies": {
    "@bervproject/feathers-advance-hook": "^1.0.4",
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@aws-sdk/client-ses": "^3.16.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^5.0.6",
    "feathers-memory": "^4.1.0",
    "feathers-sequelize": "^6.2.0",
    "helmet": "^4.6.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/nodemailer": "^6.4.1",
    "@types/sequelize": "^4.28.9",
    "@types/serve-favicon": "^2.5.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "sequelize-cli": "^6.2.0",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0",
    "yarn": ">=0.18.0"
  }
}
