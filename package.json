{
  "name": "spm-services",
  "version": "0.0.1",
  "description": "Simple Password Manager Service",
  "keywords": [
    "feathers"
  ],
  "homepage": "https://github.com/bervProject/SimplePasswordManagerService",
  "bugs": {
    "url": "https://github.com/bervProject/SimplePasswordManagerService/issues",
    "email": "bervianto.leo@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Bervianto Leo Pratama",
    "email": "bervianto.leo@gmail.com"
  },
  "contributors": [],
  "main": "src",
  "types": "lib/",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "clean": "shx rm -rf lib/",
    "compile": "yarn run clean && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --no-notify src/",
    "fix-lint": "prettier --write .",
    "jest": "jest  --forceExit",
    "lint": "prettier --check .",
    "migrate": "yarn run compile && node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "yarn run compile && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "yarn run compile && node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "start": "yarn run compile && cross-env NODE_ENV=production node lib/",
    "start:prod": "cross-env NODE_ENV=production node lib/",
    "test": "yarn run compile && yarn run jest"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.9",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.5.9",
    "@feathersjs/authentication-oauth": "^4.5.9",
    "@feathersjs/configuration": "^4.5.10",
    "@feathersjs/errors": "^4.5.9",
    "@feathersjs/express": "^4.5.9",
    "@feathersjs/feathers": "^4.5.9",
    "@feathersjs/socketio": "^4.5.9",
    "aws-sdk": "^2.787.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-advance-hook": "^0.0.8",
    "feathers-hooks-common": "^5.0.4",
    "feathers-memory": "^4.1.0",
    "feathers-sequelize": "^6.2.0",
    "helmet": "^4.2.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.15",
    "pg": "^8.4.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/cls-hooked": "^4.3.1",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nodemailer": "^6.4.0",
    "@types/sequelize": "^4.28.9",
    "@types/serve-favicon": "^2.5.1",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "axios": "^0.21.0",
    "cross-env": "^7.0.2",
    "jest": "^25.5.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "prettier-plugin-packagejson": "^2.2.7",
    "shx": "^0.3.3",
    "ts-jest": "^25.5.1",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=0.18.0"
  }
}
